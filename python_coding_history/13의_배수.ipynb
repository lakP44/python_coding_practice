    "from itertools import *\n",
    "\n",
    "a = int(input())\n",
    "a1 = list(map(int, str(a)))\n",
    "a2 = list(permutations(a1, len(a1)))\n",
    "ans = 'NO'\n",
    "for i in range (len(a2)):\n",
    "    a3 = ''.join(map(str, list(a2[i])))\n",
    "    if int(a3) % 13 == 0:\n",
    "        ans = 'YES'\n",
    "print(ans)"
   ]
  }
